{
  "crons": [
    {
      "path": "/api/cron/daily-job?trigger=market_open",
      "schedule": "30 14 * * 1-5"
    },
    {
      "path": "/api/cron/daily-job?trigger=market_close",
      "schedule": "0 21 * * 1-5"
    },
    {
      "path": "/api/cron/keep-alive",
      "schedule": "0 12 * * *"
    }
  ],
  "functions": {
    "src/app/api/stocks/backfill/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/cron/daily-job/route.ts": {
      "maxDuration": 60
    }
  }
}
